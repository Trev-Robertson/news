<%= form_tag "/login" do %>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
    <%= label_tag :password%>
    <%=password_field_tag :password%>
    <%= label_tag "Authenticator Password"%>
    <%=password_field_tag :authenticator_code%>
    <%= submit_tag "Login" %>
<% end %>
<br>
<br>
<br>
<%=link_to "Click Here to Sign Up!", '/signup' %>
